<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.*?>

<AnchorPane fx:id="anchorPane" prefHeight="80.0" prefWidth="260.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
       <Label fx:id="nameLabel" layoutX="12.0" layoutY="8.0" prefHeight="20.0" prefWidth="166.0" text="Task Name">
           <font>
            <Font size="12.0" />
           </font>
       </Label>
       <ImageView fitHeight="25.0" fitWidth="25.0" layoutX="5.0" layoutY="48.0" pickOnBounds="true" smooth="false">
           <image>
               <Image url="file:/D:/clock.png" />
           </image>
       </ImageView>
       <Label fx:id="dueLabel" alignment="CENTER" layoutX="40.0" layoutY="51.0" prefHeight="20.0" prefWidth="110.0" text="N/A">
           <font>
               <Font name="Consolas" size="10.0" />
           </font>
       </Label>
       <ImageView fitHeight="25.0" fitWidth="25.0" layoutX="187.0" layoutY="50.0" pickOnBounds="true">
           <image>
               <Image url="file:/D:/list.png" />
           </image>
       </ImageView>
       <Label fx:id="progressLabel" alignment="CENTER" layoutX="217.0" layoutY="54.0" prefHeight="17.0" prefWidth="38.0" text="N/A">
           <font>
               <Font name="Consolas" size="10.0" />
           </font>
       </Label>
       <Hyperlink alignment="CENTER" layoutX="198.0" layoutY="4.0" onAction="#onEdit" prefHeight="27.0" prefWidth="57.0" text="Edit">
           <font>
               <Font size="12.0" />
           </font>
       </Hyperlink>
       <Hyperlink alignment="CENTER" layoutX="198.0" layoutY="27.0" onAction="#onDelete" prefHeight="27.0" prefWidth="57.0" text="Delete">
           <font>
               <Font size="12.0" />
           </font>
       </Hyperlink>
      <Hyperlink alignment="CENTER" layoutX="130.0" layoutY="27.0" onAction="#onMove" prefHeight="27.0" prefWidth="57.0" text="Move">
         <font>
            <Font size="12.0" />
         </font>
      </Hyperlink>
   </children>
</AnchorPane>
